@using SKC.Entity.Models
@model SilkPayPageModel
@{
    // Layout = null;
}
<style>
    .m-portlet.m-portlet--skin-dark {
        position: relative;
    }

    .m-portlet .m-portlet__head .m-portlet__head-caption {
        width: 120px;
        white-space: nowrap;
    }

        .m-portlet .m-portlet__head .m-portlet__head-caption .m-portlet__head-title .m-portlet__head-text.head-text-nobd {
            border-bottom: none;
        }

    .m-portlet.m-portlet--skin-dark .m-portlet__head .m-portlet__head-text.head-text-nobd:after {
        width: 0;
        height: 20px;
        border-bottom: none;
        border-right: 2px solid #ddd;
        left: 100%;
        bottom: auto;
        top: 50%;
        margin-top: -8px;
    }

    .m-portlet .m-portlet__head .m-portlet__head-tools {
        padding-left: 1rem;
    }

    .m-tabs-line.nav.nav-tabs {border-bottom:none;padding-left:15px;}
    .m-tabs-line.nav.nav-tabs .nav-link {height:60px;padding-top:21px;}
    .m-tabs-line.nav.nav-tabs .nav-link:hover,
    .m-tabs-line.nav.nav-tabs .nav-link.active {border-bottom-color:#2882c8;color:#2882c8;font-weight:normal;}

    .m-portlet .m-portlet__head .sus-dropmenu {
        text-align: left;
    }

        .m-portlet .m-portlet__head .sus-dropmenu .nav-link {
            display: inline-block;
            font-size: 16px;
            padding: 0.5rem 1rem 0.3rem 1rem;
            color: #333;
        }

    .m-portlet .m-portlet__body table th {
        border-bottom: 1px solid #f1f5ff;
        background: #f1f5ff;
    }

    .m-portlet .m-portlet__head .m-portlet__head-caption {
        width: 120px;
        white-space: nowrap;
    }

    .wel-01 {
        height: 378px;
        background: url(/images/ba-silkpay/pay-wel-bg.jpg) no-repeat center right;
        background-size: cover;
    }

        .wel-01 .wel-p-area {
            float: right;
            width: 500px;
            margin-right: 100px;
            margin-top: 110px;
            color: #333;
        }

        .wel-01 .p1 {
            font-size: 22px;
        }

        .wel-01 .p2 {
            font-size: 14px;
        }

    .wel-features {
        background-color: #2882c8;
        color: #fff;
        text-align: center;
        padding-bottom: 40px;
    }

        .wel-features h4 {
            font-size: 22px;
            padding: 40px 0;
        }

        .wel-features .trait {
            position: relative;
            padding: 0 30px;
        }

            .wel-features .trait > i {
                display: block;
                width: 60px;
                height: 60px;
                margin: 0 auto;
                background-repeat: no-repeat;
                background-position: center center;
            }

            .wel-features .trait .trt-icon-01 {
                background-image: url(data:image/png;base64,);
            }

            .wel-features .trait .trt-icon-02 {
                background-image: url(data:image/png;base64,);
            }

            .wel-features .trait .trt-icon-03 {
                background-image: url(data:image/png;base64,);
            }

            .wel-features .trait .trt-icon-04 {
                background-image: url(data:image/png;base64,);
            }

            .wel-features .trait > h6 {
                font-size: 16px;
                font-weight: normal;
                margin-top: 1em;
                margin-bottom: 1em;
            }

            .wel-features .trait > p {
                font-size: 12px;
                margin-bottom: 0;
            }

            .wel-features .trait:after {
                position: absolute;
                top: 5%;
                right: 0;
                content: "";
                display: block;
                width: 0;
                height: 90%;
                border-right: 1px solid rgba(255,255,255,0.2);
            }

    .wel-03 {
        padding: 40px 0 60px 0;
        text-align: center;
    }

        .wel-03 .wel-03-tle {
            font-size: 22px;
            color: #333;
            margin-bottom: 20px;
        }

    .silk-wealth {
        padding: 30px;
    }

        .silk-wealth .wealth-wrap {
            padding-left: 42px;
        }

            .silk-wealth .wealth-wrap:before {
                content: "";
                float: left;
                width: 32px;
                height: 32px;
                background: url(data:image/png;base64,) no-repeat center center;
                margin-left: -42px;
            }

        .silk-wealth .wealth-tle {
            font-size: 16px;
            border-bottom: 1px solid #eee;
            padding: 0.35rem 0 1rem;
            margin-bottom: 2rem;
        }

        .silk-wealth .wealth-num {
            font-size: 34px;
            color: #2882c8;
        }

    .wel-result {
        position: absolute;
        left: 0;
        top: 0;
        z-index: 9;
        width: 100%;
        height: 100%;
        background-color: #fff;
        display: flex;
        justify-content: center;
        align-items: center;
        text-align: center;
        color: #919191;
    }

        .wel-result .digi-icon {
            width: 169px;
            height: 100px;
            margin: 0 auto;
        }

        .wel-result .result-h4 {
            font-size: 1.375rem;
            margin-top: 24px;
            color: #333;
        }

        .wel-result .result-msg {
            font-size: 1rem;
            height: 3em;
            line-height: 1.5;
            margin-top: 4px;
            margin-bottom: 1rem;
        }

        .wel-result .wel-success,
        .wel-result .wel-failed {
            width: 400px;
            margin: 0 auto;
        }

            .wel-result .wel-success .result-h4 {
                color: #2882c8;
            }

            .wel-result .wel-failed .result-h4 {
                color: #dfab81;
            }

            .wel-result .wel-success .digi-icon {
                background: url(data:image/png;base64,) no-repeat;
                background-size: contain;
            }

            .wel-result .wel-failed .digi-icon {
                background: url(data:image/png;base64,) no-repeat;
                background-size: contain;
            }

    @@media (max-width: 767.98px) {
        .wel-01 .wel-p-area {
            width: 100%;
            margin-right: 0;
            padding-left: 20px;
            padding-right: 20px;
        }

        .wel-features .trait {
            margin-bottom: 40px;
        }

            .wel-features .trait:last-child {
                margin-bottom: 0;
            }

        .wel-result {
            align-items: center;
            align-items: flex-start;
        }

            .wel-result .wel-success,
            .wel-result .wel-failed {
                width: 100%;
                margin-top: 10rem;
            }

        .sus-m-tabs-lap {display:none;}
        .sus-dropmenu {display:block;}
    }
    @@media (min-width: 768px) {
        .sus-m-tabs-lap {display:flex;}
        .sus-dropmenu {display:none;}
    }
</style>
<div class="m-grid__item m-grid__item--fluid m-wrapper">
    <div class="m-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="m-portlet m-portlet--mobile m-portlet--skin-dark">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <a class="m-portlet__head-title" href="/SilkPay/Index"><h3 class="m-portlet__head-text head-text-nobd active"><span class="font-weight-bold">@Html.Raw(Html.GetTansByLan("silkpay-index-01", "Silk Pay"))</span></h3></a>
                        </div>
                        <div class="m-portlet__head-tools">
                            <ul class="nav nav-tabs m-tabs m-tabs-line m-tabs-line--left sus-m-tabs-lap" id="profile_tab" role="tablist">
                                @if (Model.SilkPayStatus)
                                {
                                <li class="nav-item m-tabs__item"><a class="nav-link m-tabs__link" href="/SilkPay/Wallet">@Html.Raw(Html.GetTansByLan("silkpay-index-02", "我的钱包"))</a></li>
                                <li class="nav-item m-tabs__item"><a class="nav-link m-tabs__link" href="/SilkPay/Recharge">@Html.Raw(Html.GetTansByLan("silkpay-index-03", "充币"))</a></li>
                                <li class="nav-item m-tabs__item"><a class="nav-link m-tabs__link" href="/Wallet/Index">@Html.Raw(Html.GetTansByLan("silkpay-index-04", "提币"))</a></li>
                                <li class="nav-item m-tabs__item"><a class="nav-link m-tabs__link" href="/SilkPay/Transaction">@Html.Raw(Html.GetTansByLan("silkpay-index-05", "资金记录"))</a></li>
                                <li class="nav-item m-tabs__item"><a class="nav-link m-tabs__link" href="https://api-silk-1000.silkchain.io/Home/Index" target="_blank">@Html.Raw(Html.GetTansByLan("silkpay-index-06", "开发对接"))</a></li>
                                }
                            </ul>
                            <div class="sus-dropmenu">
                                <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">@Html.Raw(Html.GetTansByLan("silkpay-index-02", "我的钱包"))</a>
                                <div class="dropdown-menu">
                                    @if (Model.SilkPayStatus)
                                    {
                                    <a class="dropdown-item" href="/SilkPay/Index">@Html.Raw(Html.GetTansByLan("silkpay-index-02", "我的钱包"))</a>
                                    <a class="dropdown-item" href="/SilkPay/Recharge">@Html.Raw(Html.GetTansByLan("silkpay-index-03", "充币"))</a>
                                    <a class="dropdown-item" href="/Wallet/Index">@Html.Raw(Html.GetTansByLan("silkpay-index-04", "提币"))</a>
                                    <a class="dropdown-item" href="/SilkPay/Transaction">@Html.Raw(Html.GetTansByLan("silkpay-index-05", "资金记录"))</a>
                                    <a class="dropdown-item" target="_blank"  href="https://api-silk-1000.silkchain.io/Home/Index">@Html.Raw(Html.GetTansByLan("silkpay-index-06", "开发对接"))</a>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body m-portlet__body--no-padding">
                            <div class="wel-01">
                                <div class="wel-p-area">
                                    <p class="p1">@Html.Raw(Html.GetTansByLan("silkpay-index-07", "随着您业务的发展支付方式也如此，使用SILKPAY开拓您的网上业务吧!"))</p>
                                    <p class="p2">@Html.Raw(Html.GetTansByLan("silkpay-index-08", "支付产品多样化：专注全球收付款（银行转账、国际电汇）、法币及Silk支付、社交红包等等"))</p>
                                </div>
                            </div>
                            <div class="wel-features">
                                <h4>@Html.Raw(Html.GetTansByLan("silkpay-index-09", "优惠，高效，安全，合规"))</h4>
                                <div class="row">
                                    <div class="col-12 col-md-3 trait">
                                        <i class="trt-icon-01"></i>
                                        <h6>@Html.Raw(Html.GetTansByLan("silkpay-index-10", "费用低"))</h6>
                                        <p>@Html.Raw(Html.GetTansByLan("silkpay-index-11", "收付款免费，为您节省更多。"))</p>
                                    </div>
                                    <div class="col-12 col-md-3 trait">
                                        <i class="trt-icon-02"></i>
                                        <h6>@Html.Raw(Html.GetTansByLan("silkpay-index-12", "高效"))</h6>
                                        <p>@Html.Raw(Html.GetTansByLan("silkpay-index-13", "简单动下手指，您的交易即可<br />安全完成，可随时随地"))<br />@Html.Raw(Html.GetTansByLan("silkpay-index-14", "完成任何交易。"))</p>
                                    </div>
                                    <div class="col-12 col-md-3 trait">
                                        <i class="trt-icon-03"></i>
                                        <h6>@Html.Raw(Html.GetTansByLan("silkpay-index-15", "安全合规"))</h6>
                                        <p>@Html.Raw(Html.GetTansByLan("silkpay-index-16", "持有PCI&amp;DSS证书，"))<br />@Html.Raw(Html.GetTansByLan("silkpay-index-17", "KYC, AML/CFT 认证。"))</p>
                                    </div>
                                    <div class="col-12 col-md-3 trait">
                                        <i class="trt-icon-04"></i>
                                        <h6>@Html.Raw(Html.GetTansByLan("silkpay-index-18", "包容性"))</h6>
                                        <p>@Html.Raw(Html.GetTansByLan("silkpay-index-19", "SilkPay可为您不断发展的"))<br />@Html.Raw(Html.GetTansByLan("silkpay-index-20", "业务专业定制支付解决方案，"))<br />@Html.Raw(Html.GetTansByLan("silkpay-index-21", "使用SILKPAY开拓您的网上业务吧。"))</p>
                                    </div>
                                </div>
                            </div>
                            @if (!Model.SilkPayStatus) { 
                                <div class="wel-03">
                                    <div class="wel-03-tle">@Html.Raw(Html.GetTansByLan("silkpay-index-22", "管理您的资金"))</div>
                                    <a class="btn btn-primary m-btn--pill" href="javascript:;" onclick="OpenUserSilkPay()"> &nbsp; @Html.Raw(Html.GetTansByLan("silkpay-index-23", "免费开设Silk Pay帐户")) &nbsp; </a>
                                </div>
                            }
                            <div class="wel-result" style="display:none;">
                                <div class="wel-success">
                                    <div class="digi-icon"></div>
                                    <div class="result-h4">@Html.Raw(Html.GetTansByLan("silkpay-index-24", "开通成功"))</div>
                                    <p class="result-msg"></p>
                                    <a class="btn btn-outline-focus m-btn--pill" href="/SilkPay/Index">@Html.Raw(Html.GetTansByLan("silkpay-index-25", "立即进入"))</a>
                                </div>
                            </div>
                            <div class="wel-result" style="display:none;">
                                <div class="wel-failed">
                                    <div class="digi-icon"></div>
                                    <div class="result-h4">@Html.Raw(Html.GetTansByLan("silkpay-index-26", "SILKPAY 开通失败"))</div>
                                    <p class="result-msg">@Html.Raw(Html.GetTansByLan("silkpay-index-27", "请再次尝试或联系客服处理"))</p>
                                    <a class="btn btn-outline-focus m-btn--pill" href="/SilkPay/Index"> &nbsp; @Html.Raw(Html.GetTansByLan("silkpay-index-28", "返回")) &nbsp; </a>
                                </div>
                            </div>
                            <div>
                                官方应用开发者：
                                @if (Model.AuthorizeList != null && Model.AuthorizeList.Count() > 0) {
                                    foreach (var item in Model.AuthorizeList) { 
                                        <div>应用名：@(item.DisplayName??"暂无") Logo地址：@(item.LogoUrl) 应用介绍：@item.Description</div>
                                    }
                                }
                            </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section script{
    <script src="~/js/loading.js"></script>
    <script>
    function OpenUserSilkPay() {
        $.showLoading('@Html.Raw(Html.GetTansByLan("packet-send-34", "Waiting"))');
        $.post('/SilkPay/OpenSilkPayment', function (data) {
            if (data == 1) {
                $.hideLoading();
                $(".wel-success").parent().show();
            } else {
                $.hideLoading();
                $(".wel-failed .result-msg").text(data);
                $(".wel-failed").parent().show();
            }
        });
    }
    </script>
}
