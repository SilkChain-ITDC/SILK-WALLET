function calcUsdRate(seft, idOutPut, usdExchage , min ,currency) {
    var reg_number = /^[0-9]+(\.[0-9]{1,})?$/;
    var value = $(seft).val();
    if(value != '' && !reg_number.test(value.trim())){
        $(seft)[0].setCustomValidity('Invalid amount');
        $(idOutPut).removeAttr('disabled').val('').attr('disabled', 'disabled');
    }else if(value.trim() < min){
        $(seft)[0].setCustomValidity('Minimum '+currency+' per transaction is '+min);
        $(idOutPut).removeAttr('disabled').val('').attr('disabled', 'disabled');
    }else{
        $(seft)[0].setCustomValidity('');
        $(idOutPut).removeAttr('disabled').val(number_format(value * usdExchage, 2)).attr('disabled', 'disabled');
    }
}

