    //ViewData["Title"] = "Index";
    var ETH = Model.FirstOrDefault(c => c.WalletInfo.CoinTypeNvarchar == "ETH");
    var BTC = Model.FirstOrDefault(c => c.WalletInfo.CoinTypeNvarchar == "BTC");
    var SKCModel = Model.FirstOrDefault(c => c.WalletInfo.CoinTypeNvarchar == @SKC.Common.GlobalConfig.Coin_ShortName);
    int user_status = int.Parse(ViewData["KYC"].ToString());
    string target = "#m_withdraw_silk";
    ViewBag.Title = "SilkChain - Withdraw";
    var WithDraw = ViewBag.WithdrawData as List<SkcWithdraw>;
}

<style>
	.sub-title {
		font-size: 16px !important;
	}

	.sub-title-18 {
		font-size: 18px !important;
	}

    .table td {white-space:nowrap;}
    .m--font-danger {color:#ff7800 !important;}
    .form-control[readonly], .form-control {border-color:#ccc;font-size:16px;padding:0.72rem 1.25rem;}
    .form-control[disabled] {border-color:#ccc;}
    .input-group .input-group-addon {border-color:#ccc;background-color:#fff;}
    .form-control ~ .input-group-addon:not(:first-child) {border-left:0;}
	.display-table {
		display: table;
		width: 100%;
	}

		.display-table > div {
			display: table-cell;
			vertical-align: middle;
		}

	.desc-info {
		color: #999999;
		font-size: 12px;
	}

	.des-deposit span {
		display: block;
	}
    .des-deposit .font-size-18 strong {font-size:18px;color:#2485c9;margin-left:4px;vertical-align:initial;}

    .alert-wrap {padding:0 8px;background-color:#f7f8fa;}
    @@media (min-width:992px){
        .display-table.width-auto {width:auto !important;}
        .width-lg-max {max-width:480px;}
    }

	.m-tabs-line a.m-tabs__link {
		font-size: 16px;
	}

	.m-tabs-line a.m-tabs__link {
		text-transform: uppercase;
	}

	.p-content {
		font-weight: 400;
	}

	.txId {
		display: inline-block;
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipses;
		width: 200px;
		margin-right: 10px;
	}

	.table-responsive {
		position: relative;
        min-height:250px;
	}
    @@media (max-width: 767.98px) {
        .alert-wrap {padding:0px;}
        .des-deposit .font-size-18 strong {font-size:16px;margin-left:0px;font-weight:500;}
	}
    .alert-notify-card {position:absolute;right:15px;margin-top:6px;margin-bottom:0;}
    .alert-notify-card .close { float: right; position: absolute; right: 10px; top: 0px; }
    .alert-notify-card .alert-cont {word-break: break-all; word-wrap: break-word; }
    .alert-notify-card:before { position: absolute; right:30px; top: -9px; content: ""; margin-left:-8px; display: block; width: 0; height: 0; overflow: hidden; border-left: 8px solid transparent; border-right: 8px solid transparent; border-bottom: 8px solid #c6c8ca; }
</style>
