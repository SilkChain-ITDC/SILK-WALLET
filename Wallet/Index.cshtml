$(document).ready(function () {
    $(".m-nav__item a").click(function (e) {
        var url = $(this).attr('href');
        var hash = url.substring(url.indexOf('#'));
        depositHash(hash);
    });

    $('#wallet_tabs a').click(function (e) {
        var id = $(e.target).attr("href").substr(1);
        id = id.substr(0, id.length - 4);
        window.location.hash = id;
        e.preventDefault();
        $(this).tab('show');
        if(id =='m_tab_btc'){
            getIcoInfo("repeat");
            reloadBtc();
        }else if(id =='m_tab_eth'){
            getIcoInfo("repeat");
            reloadEth();
        }else if(id =='m_tab_ezt'){
            getIcoInfo("repeat");
            reloadEzt();
        }
    });

    depositHash(window.location.hash);

    function depositHash(hash) {
