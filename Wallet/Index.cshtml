                            <div style="padding:0px 0px 15px 0px">
                                <div class="table-responsive">
                                    <table class="table" style="width: 100%">
                                        <thead>
                                            <tr>
                                                <th>@Html.GetTansByLan("recharge-purchase-tab-17", "Date")</th>
                                                <th class="text-center">@Html.GetTansByLan("recharge-purchase-tab-18", "RECIPIENT ADDRESS")</th>
                                                <th class="text-center">@Html.GetTansByLan("recharge-purchase-tab-19", "STATUS  ")</th>
                                                <th class="text-center">@Html.GetTansByLan("recharge-purchase-tab-20", "AMOUNT")</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{ 
                                                List<TypeModel> withDrawType = new List<TypeModel>();
                                                foreach (var item in Enum.GetValues(typeof(EnumWidthDraw)))
                                                {
                                                    string temp_str = string.Format("wallet-index-type-{0}", (int)item);
                                                    withDrawType.Add(new TypeModel { Key = (int)item, Params = temp_str, Text = item.ToString() });
                                                }
                                            }
                                            @if (WithDraw != null && WithDraw.Count > 0)
                                            {
                                                foreach (var item in WithDraw)
                                                {
                                                    var temp_item = withDrawType.FirstOrDefault(p => p.Key == item.StatusInt);
                                                    <tr>
                                                        <td>@(item.CreateTimeDatetime.ToString("HH:mm MM/dd"))</td>
                                                        <td>@(item.CoinAddressNvarchar)</td>
                                                        <td>@(temp_item!=null? Html.GetTansByLan(temp_item.Params,temp_item.Text): Html.GetTansByLan("wallet-index-type-default", item.StatusNameInt))</td>
                                                        <td>@(item.AmountDecimal.ToString("n2"))@(item.CoinTypeNvarchar)</td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

