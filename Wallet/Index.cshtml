$("#btn_silk_withdraw").on('click', function(e){
    e.preventDefault();
    var form = $(this).closest('form');
    var hasError = false;
    var val;

    var kyc = '@user_status';
    if (kyc != 1) {
        hasError = true;
        $('#m_show_msg_kyc').modal('show');
        return;
    }

    val = form.find('input[name="WithdrawalAddress"]').val();
    if (!(val != '' && web3.utils.isAddress(val))) {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-2", "Please Check Whether the Address Correct or Not."))');
        return;
    }
    val = $.trim(form.find('input[name="WithdrawalAmount"]').val());
    var balance_amount = '@SKCModel.WalletInfo.AmountBalanceDecimal';
    var regPos = /^\d+(\.\d+)?$/;

    if (val == "") {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-3", "Please check amount correctly!"))');
    }
    if (!regPos.test(val)) {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-3", "Please check amount correctly!"))');
    }
    if (parseFloat(val) == 0) {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-4", "Insufficient balance!"))');
    }
    if (parseFloat(balance_amount) < parseFloat(val)) {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-4", "Insufficient balance!"))');
    }
    if (parseFloat(val) < 2000) {
        hasError = true;
        $('#silk_withdraw_error').html('@Html.Raw(Html.GetTansByLan("wallet-index-error-tip-5", "It must be more than 2000!"))');
    }

    if (!hasError) {
        $("#form_withdraw_silk").trigger('submit');
    }
});
