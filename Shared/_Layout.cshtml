					if (typeof analytics !== "undefined") {analytics.track("Deposit", {currency:"ETH"});}
				});
			}
			function onDepositEzt() {
                new Promise(function(resolve, reject){
                    setTimeout(function(){
                        resolve('0xc35b238546a6b02b2d2bd996ecdae322f9f8120b');
                    }, 80);
				}).then(function(data) {
					if(data !=  ''){
                        $("#img_qr_ezt_address").empty().qrcode({width:320,height:320,text:"https://etherscan.io/token/"+data});
						$("#tx_ezt_address").val(data);
						$("#m_deposit_ezt").modal("show");
						if (typeof analytics !== "undefined") {analytics.track("Deposit", {currency:"EZT"});}
					}
				});
			}
            function createEvt(obj){
                var evt = document.createEvent('MouseEvents');
                evt.initEvent("click", true, true);
                obj.dispatchEvent(evt);
            }
			function copyValToClipboard(element) {
				// var $temp = $("<input>");
				// $(element).parent().append($temp);
				// $temp.val($(element).val()).select();
				// document.execCommand("copy");
				// $temp.remove();

                var trig = $(element).get(0);
                if (!trig.slipboardinit){
                    trig.slipboardinit = true;
                    var clipboard = new ClipboardJS(trig);
                    createEvt(trig);
                }
                $(element).tooltip({
                    title: 'Copy Success!',
                    placement: 'top',
                    trigger: 'click',
                    delay: {"show": 500, "hide": 100}
                }).tooltip('show');
                setTimeout(function(){
                    $(element).tooltip('hide');
                }, 800);

				return false;
			}
            </script>
