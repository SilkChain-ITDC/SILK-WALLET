@section script{
    <script src="~/js/sweetalert.min.js"></script>
    <script src="~/js/bignumber.js"></script>
    <script type="text/javascript">
        function getCookie(c_name) {
            if (document.cookie.length > 0) {
                c_start = document.cookie.indexOf(c_name + "=");
                if (c_start != -1) {
                    c_start = c_start + c_name.length + 1
                    c_end = document.cookie.indexOf(";", c_start)
                    if (c_end == -1) c_end = document.cookie.length
                    return unescape(document.cookie.substring(c_start, c_end))
                } else {
                    return "";
                }
            }
            return ""
        }
        function setCookie(c_name, value, expiredays) {
            var exdate = new Date()
            exdate.setDate(exdate.getDate() + expiredays)
            document.cookie = c_name + "=" + escape(value) + ";expires=" + exdate.toGMTString() + ";path=/;domain=silkchain.io";
            document.cookie = c_name + "=" + escape(value) + ";expires=" + exdate.toGMTString() + ";path=/;domain=silkb2b.io";
        }
        $(function () {
            if (getCookie("_2fa") == "") {
                $("#m_modal_prompt_2fa").modal();
                setCookie('_2fa', 1, 1);
                return false;
            }
            return false;
        })
        var coin_price_range = @Html.Raw(JsonConvert.SerializeObject(coin_price_range));
        var now_rates = @Html.Raw(JsonConvert.SerializeObject(now_rare));
        var now_promotion = @Html.Raw(now_promotion != null ? JsonConvert.SerializeObject(now_promotion) : "{}");
        var has_bonus = $.isEmptyObject(now_promotion) ? false : true;
        var buy_silk = {
            params: { eth_silk_rate: 1, url: '/Recharge/CheckSkcPublish',rate:[] },
            inItControl: function () {
                var reg_num = /^\d+(\.\d+)?$/;
                var error_num_tag = $("#ezt_withdraw_error"), eth_count_tag = $("#lblEztRemaining");

                buy_silk.bind_info(now_rates);

                $("#ezt_amount").keyup(function () {
                    var eth_val = $(this).val();
                    var coin_type = $("#form_buy_silk").find('input[type=radio][name=CoinType]:checked').val();
                    var now_rate = buy_silk.get_cointype_rate(coin_type);
                    var chk_range_price = buy_silk.chk_price_range(reg_num, coin_type, eth_val);
                    $("#sp_choose_cointype").html(coin_type);
                    if (reg_num.test(eth_val)) {
                        var silk_count_num = Math.ceil(eth_val * now_rate);
                        var silk_num = new BigNumber((silk_count_num).toString());
                        $("#hd_silk_num").val(silk_count_num);
                        eth_count_tag.val(silk_num.toFormat());
                        if (has_bonus) {
                            var silk_bonus = new BigNumber(0);
                            if (silk_count_num >= now_promotion.MOQ) {
                                var r = new BigNumber(now_promotion.PromotionRatio);
                                silk_bonus = silk_num.multipliedBy(r);
                            }
                            var silk_total = silk_num.plus(silk_bonus);
                            $("#txt_bonus").val(silk_bonus.toFormat());
                            $("#strong_total").text(silk_total.toFormat());
                        }
                    }
                    if (chk_range_price.ok) {
                        error_num_tag.hide();
                    } else {
                        error_num_tag
                            .find(".lb_coin_type").html(coin_type).parent()
                            .find(".lb_min_price").html(chk_range_price.min_p).parent()
                            .find(".lb_max_price").html(chk_range_price.max_p).parent()
                            .show();
                    }
                }).blur(function () { $(this).keyup(); });

                $("#btn_ezt_withdraw").click(function () {
                    var eth_tag = $("#ezt_amount");
                    var coin_type = $("#form_buy_silk").find('input[type=radio][name=CoinType]:checked').val();
                    var chk_range_price = buy_silk.chk_price_range(reg_num, coin_type, eth_tag.val());
                    if (chk_range_price.ok) {
                        var url_params = $("#form_buy_silk").serializeArray(); //{ Option: 'Platform', SilkNum: eth_tag.val(), CoinType: coin_type, RealTimePrice: buy_silk.params.eth_silk_rate };
                        url_params.push({ name: 'Option', value: 'Platform' });
                        buy_silk.request_data(buy_silk.params.url, url_params, { Option: 'Platform' });
                    }
                });

                $("#but_ok").click(function () {
                    location.reload();
                });

                $("#form_buy_silk").find('input[type=radio][name=CoinType]').click(function () {
                    var coin_type = $(this).val();
                    var rate = buy_silk.get_cointype_rate(coin_type);
                    var silk = new BigNumber(rate.toString());
                    // $("#sp_nowsilk").html(silk.toFormat());
                    $("#sp_now_cointype").html(coin_type).parent().attr('class', 'font-size-32 '+ coin_type.toLowerCase() +'-color');
                    $("#ezt_amount").blur();
                });
            },
            request_data: function (url, params, opt) {
                $.ajax({
                    url: url,
                    data: params,
                    type: 'post'
                }).done(function (data) {
                    $(".m-loader").hide();
                    if (data.result) {
                        switch (params.Option || opt.Option) {
                            case "Platform":
                                $("#btnResetEzt").click();
                                $("#sp_payment").html(data.data.Amount + " " + data.data.CoinType);
                                $("#deposit_silk_address").val(data.data.Address);
                                $("#img_qr_silk_address").empty().qrcode({ width: 240, height: 240, text: data.data.Address });
                                $("#m_eth_address").modal('show');

                                $("#m_eth_address").on('hide.bs.modal', function () {
                                    window.location.reload();
                                });
                                break;
                            case "CancelOrder":
                                swal({
                                    text: data.message,
                                    type: "success",
                                }).then(function (value) {
                                    location.reload();
                                });
                                break;
                            case "GetNowRate":
                                buy_silk.bind_info(data.data);
                                // $("#m_deposit_silk_useeth").modal('show');
                                break;
                            default:
                                break;
                        }
                    } else {
                        swal({
                            text: data.message == undefined ? "Please login again" : data.message,
                            type: data.result ? "success" : "error",
                        }).then(function (value) {
                        });
                    }
                }).fail(function (xhr, status, error) {
                    //alert("system error");
                    location.reload();
                });
            },
            show_pay_address: function (obj) {
                var address = $(obj).attr("data-address");
                var coin_type = $(obj).attr("data-cointype");
                var amount = $(obj).attr("data-dealamount");
                $("#sp_payment").html(amount + " " + coin_type);
                $("#deposit_silk_address").val(address);
                $("#img_qr_silk_address").empty().qrcode({ width: 240, height: 240, text: address });
                $("#m_eth_address").modal('show');
            },
            cancel_order: function (obj) {
                var id = $(obj).attr("data-id");
                swal({
                    title: "@Html.Raw(Html.GetTansByLan("recharge-purchase-40", "Cancel Order"))",
                    text: "",
                    icon: "warning",
                    buttons: true,
                    dangerMode: true,
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'Confirm'
                }).then(function(willDelete){
                    if (willDelete) {
                        var url_params = { Option: 'CancelOrder', "ID": id };
                        buy_silk.request_data(buy_silk.params.url, url_params);
                    } else {
                    }
                });
            },
            bind_info: function (data) {
                buy_silk.params.rate = data;
                var silk_num = 0;
                if (buy_silk.params.rate != "" && buy_silk.params.rate.length > 0) {
                    $.each(buy_silk.params.rate, function (i, n) {
                        if (n.CoinType == "ETH") {
                            silk_num = new BigNumber(n.Price.toString());
                            $(".sp_ethtosilk").html(silk_num.toFormat());
                        } else if (n.CoinType == "USDT"){
                            silk_num = new BigNumber(n.Price.toString());
                            $(".sp_usdttosilk").html(silk_num.toFormat());
                        } else if (n.CoinType == "BTC") {
                            silk_num = new BigNumber(n.Price.toString());
                            $(".sp_btctosilk").html(silk_num.toFormat());
                        }
                    });
                }
                $("#ezt_amount").blur();
                $("#form_buy_silk").find('input[type=radio][name=CoinType]:checked').click();
            },
            get_cointype_rate: function (type) {
                var rate = 0;
                $.each(buy_silk.params.rate, function (i, n) {
                    if (n.CoinType == type) {
                        rate = n.Price;
                        return false;
                    }
                });
                return rate;
            },
            chk_price_range: function (regx, coin_type, val) {
                var temp_price = { ok: false, min_p: '', max_p: '' };
                $.each(coin_price_range, function (i, n) {
                    if (n.CoinType == coin_type) {
                        temp_price.min_p = n.MinPrice;
                        temp_price.max_p = n.MaxPrice;
                        if (regx.test(val) && (val >= n.MinPrice && val <= n.MaxPrice)) {
                            temp_price.ok = true;
                        }
                        return false;
                    }
                });
                return temp_price;
            }
        };
