@using SKC.Entity.DBModels
@using SKC.Entity.Models
@using SKC.Entity.Enums
@model PacketsRecPageModel
@{
    Layout = null;
    List<ReceListPageModel> receList = Model.ReceList;
    List<SkcRedPacket> sendList = Model.SendList;
    List<SkcInOutRecord> inOutRecordList = Model.InOutRecordList;
    string Lan = ViewBag.Lan as string;
    string UserId = ViewBag.UserId as string;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="/css/vendors.stripped.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="/css/style.bundle.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="/css/main.css" rel="stylesheet" asp-append-version="true" />
    <link rel="shortcut icon" href="/favicon.ico" />
</head>
<style>
    .m-grid.m-grid--ver-desktop.m-grid--desktop {
        display: flex;
        flex-direction: row;
    }

        .m-grid.m-grid--ver-desktop.m-grid--desktop > .m-grid__item.m-grid__item--fluid {
            flex: 1 0 auto;
        }

    .rowitem-none {
        text-align: center;
        padding: 1rem 0;
    }

    .m-content,
    .m-packet__container {
        height: 100%;
    }

    .m-packet__wrapper {
        padding-top: 20px;
        width: 430px;
        margin: 0 auto;
    }

    .m-stack.m-stack--flex-general .m-packet__wrapper {
        flex: 1 0 auto;
    }

    .m-packet__wrapper .red-packet {
        position: relative;
        padding: 30px 0;
        text-align: center;
        color: #333;
    }

    .m-packet__wrapper .avatar {
        display: flex;
        align-items: center;
        justify-content: center;
        width: 4rem;
        height: 4rem;
        margin: 0 auto;
        border-radius: 0.4285714rem;
        border: 1px solid #feb041;
        background-color: #fff;
    }

        .m-packet__wrapper .avatar > img {
            max-width: 100%;
            max-height: 100%;
        }

    .m-packet__wrapper .sendname {
        font-size: 1.15rem;
        margin: 1rem 0 0.3rem 0;
    }

    .m-packet__wrapper .sendopt {
        font-size: 1rem;
    }

    .m-packet__wrapper .price {
        margin-top: 10px;
        margin-bottom: 4px;
        line-height: 1;
    }

        .m-packet__wrapper .price > strong {
            font-size: 3rem;
            font-family: arial;
        }

        .m-packet__wrapper .price > em {
            font-size: 1.15rem;
            font-style: normal;
        }

    .m-packet__wrapper .total {
        margin-top: 20px;
        margin-left: 30px;
        margin-right: 30px;
    }

        .m-packet__wrapper .total .total-numb {
            color: #888;
            font-size: 1em;
        }

            .m-packet__wrapper .total .total-numb strong {
                display: block;
                font-weight: normal;
                font-size: 2.4rem;
                line-height: 1;
            }

    .m-packet__wrapper .btn-outline-danger {
        background-color: #fff;
        color: #ff9600;
        border-color: #ff9600;
    }

        .m-packet__wrapper .btn-outline-danger:hover {
            background-color: #ff9600;
            color: #fff;
        }

    .m-packet__wrapper .total .btn-outline-danger {
        margin-top: 4px;
        padding-left: 3.25rem;
        padding-right: 3.25rem;
    }

    .m-packet__wrapper .date-filter {
        position: absolute;
        top: 0px;
        right: 20px;
    }

        .m-packet__wrapper .date-filter .filter-control-select {
            border: none;
            background: none;
            padding-right: 0 \9;
            background: none \9;
            outline: none;
            color: #ff9400;
            appearance: none;
            -moz-appearance: none;
            -webkit-appearance: none;
        }

        .m-packet__wrapper .date-filter .arrow-btm {
            font-size: 0;
            text-align: center;
        }

            .m-packet__wrapper .date-filter .arrow-btm:after {
                display: inline-block;
                width: 8px;
                height: 8px;
                overflow: hidden;
                content: "";
                border-right: 1px solid #ff9400;
                border-bottom: 1px solid #ff9400;
                transform: rotate(45deg);
            }

    .m-packet__wrapper .draw-packet {
        position: relative;
        background-color: #fff;
        flex: 1 0 auto;
        overflow: hidden;
    }

    .m-packet__wrapper .draw-packet-inner {
        width: 100%;
        height: 100%;
    }

    .m-packet__wrapper .draw-packet-tle {
        padding: 10px 15px;
        border-bottom: 1px solid #ddd;
        font-size: 1rem;
        color: #888;
    }

    .m-packet__wrapper .rowitem {
        border-bottom: 1px solid #ddd;
        padding: 10px 0;
        position: relative;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-align-items: flex-star;
        align-items: flex-star;
    }

        .m-packet__wrapper .rowitem:last-child {
            border-bottom: none;
        }

    .m-packet__wrapper .rowitem-has-operate .rowitem:nth-child(2n+1) {
        border-bottom: 1px dotted #f0f0f0;
    }

    .m-packet__wrapper .rowmd {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
        flex: 1;
    }

    .m-packet__wrapper .rowitem-ul {
        padding: 0 15px;
    }

    .m-packet__wrapper .nametxt {
        font-size: 1.15rem;
        line-height: 1;
        margin-bottom: 0.4rem;
    }

    .m-packet__wrapper .datenub {
        font-size: 1em;
        color: #bbb;
        line-height: 1;
    }

    .m-packet__wrapper .distrib {
        text-align: right;
        line-height: 1;
    }

    .m-packet__wrapper .linefeed {
        margin-top: 0.5rem;
    }

    .m-packet__wrapper .text-orange {
        color: #ff9600;
    }

    .m-packet__wrapper .text-gray {
        color: #bbb;
    }

    @@media (max-width: 767.98px) {
        .m-packet__wrapper {
            width: 100%;
            padding-top: 0;
        }

        .m-body .m-content {
            padding: 0;
        }

        .m-packet__wrapper .red-packet {
            background-color: #f0f0f0;
        }

        .m-content--skin-light2 .m-body {
            background-color: #fff;
        }

        .m-packet__wrapper .red-packet {
            padding-top: 50px;
        }

        .m-packet__wrapper .date-filter {
            top: 20px;
        }
    }

    @@media (min-width: 992px) {
        .m-packet__wrapper {
            background-color: #fff;
        }

            .m-packet__wrapper .rowitem:first-child {
                border-top: 1px solid #ddd;
            }
    }
</style>
<body class="m-grid m-grid--hor m-content--skin-light2">
<div class="m-grid__item m-grid__item--fluid m-wrapper m-body">
    <div class="m-content">
        @if (Model.PacketType == "rece")
        {
            <div class="m-packet__container m-stack m-stack--flex-general">
                <div class="m-packet__wrapper">
                    <div class="red-packet">
                        <div class="avatar"><img width="80%" src="data:image/png;base64," /></div>
                        <div class="sendname"><span>@Model.UserName</span>@Html.GetTansByLan("packet-packetList-01", " Received", Lan)</div>
                        <div class="price"><strong>@Model.Amount.ToString("0.00")</strong></div>
                        <div class="row total" style="margin-left:0;margin-right:0;">
                            <div class="col col-6">
                                <div class="total-numb">
                                    <strong>@Model.Total</strong>@Html.GetTansByLan("packet-packetList-02", "Total Number", Lan)
                                </div>
                            </div>
                            <div class="col col-6">
                                <div class="total-numb">
                                    <strong>@Model.Luckiest_count</strong>@Html.GetTansByLan("packet-packetList-03", "Luckiest Draw", Lan)
                                </div>
                            </div>
                        </div>
                        <div class="row total" style="margin-left:0;margin-right:0;">
                            <div class="col col-6" style="padding:0;">
                                <div class="total-numb">
                                    <strong>@inOutRecordList.Count()</strong>@Html.GetTansByLan("packet-packetList-04", "Shared Packets", Lan)
                                </div>
                            </div>
                            <div class="col col-6" style="padding:0;">
                                <div class="total-numb">
                                    <strong>@((inOutRecordList.Sum(t => t.AmountDecimal)).ToString("0.00"))<small style="font-size:40%;">@Html.GetTansByLan("packet-silk-alias-01", "SILK", Lan)</small></strong>@Html.GetTansByLan("packet-packetList-05", "Bonus by Sharing", Lan)
                                </div>
                            </div>
                        </div>
                        <div style="display:none" id="lucky-money-type">@Model.PacketType</div>
                        <div class="date-filter">
                            <select class="filter-control-select" onchange="OnChange()">
                                @if (Model.Years == "2019")
                                {
                                    <option value="2019" selected="selected">@Html.GetTansByLan("packet-packetList-08", "2019 years", Lan)</option>}
                                else
                                {
                                    <option value="2019">@Html.GetTansByLan("packet-packetList-08", "2016 years", Lan)</option>}
                                @if (Model.Years == "2018")
                                {
                                    <option value="2018" selected="selected">@Html.GetTansByLan("packet-packetList-06", "2018 years", Lan)</option> }
                                else
                                {
                                    <option value="2018">@Html.GetTansByLan("packet-packetList-06", "2018 years", Lan)</option>
                                }
                                @if (Model.Years == "2017")
                                {
                                    <option value="2017" selected="selected">@Html.GetTansByLan("packet-packetList-07", "2017 years", Lan)</option>}
                                else
                                {
                                    <option value="2017">@Html.GetTansByLan("packet-packetList-07", "2017 years", Lan)</option>}
                            </select>
                            <div class="arrow-btm"></div>
                        </div>
                    </div>
                    <div class="draw-packet">
                        <div class="draw-packet-inner">
                            <div class="rowitem-ul">

                                @if (receList != null && receList.Count > 0)
                                {
                                    foreach (var item in receList)
                                    {
                                        <div class="rowitem">
                                            <div class="rowmd">
                                                <div class="nametxt">@item.SendUserName</div>
                                                <div class="datenub">@item.Data.CreateTimeDatetime.ToString("MM-dd HH:mm")</div>
                                            </div>
                                            <div class="rowft">
                                            <div class="distrib">@item.Data.AmountDecimal.ToString("0.00") @Html.GetTansByLan("packet-silk-alias-01", "SILK", Lan)<div class="linefeed">
                                                <a href="/Affirm/GetPacketIndex?OnlyCode=@item.OnlyCode&from=2&UserId=@(UserId)&Lan=@Lan">@Html.GetTansByLan("packet-packetList-09", "check details", Lan)</a>
                                                    </div></div></div>
                                        </div>
                                    }
                                }
                                else
                                {
                                    <div class="rowitem-none">
                                        @Html.GetTansByLan("packet-packetList-10", "No data available", Lan)
                                    </div>
                                }

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
        else
        {
            <div class="m-packet__container m-stack m-stack--flex-general">
                <div class="m-packet__wrapper">
                    <div class="red-packet">
                        <div class="avatar"><img width="80%" src="data:image/png;base64," /></div>
                        <div class="sendname"><span>@Model.UserName</span>@Html.GetTansByLan("packet-packetList-11", " total sent", Lan)</div>
                        <div class="price"><strong>@Model.Amount.ToString("0.00")</strong></div>
                        <div class="total">
                            <div class="send-sum">@Html.GetTansByLan("packet-packetList-19", "sent", Lan)<span class="text-orange">@sendList.Count()</span>@Html.GetTansByLan("packet-packetList-12", " red packets", Lan)</div>
                            @*<a class="btn btn-outline-danger m-btn--pill" href="/Packet/SendluckyMoney">@Html.GetTansByLan("packet-packetList-13", "I want to send a red packet", Lan)</a>*@
                        </div>
                        <div style="display:none" id="lucky-money-type">@Model.PacketType</div>
                        <div class="date-filter">
                            <select class="filter-control-select" onchange="OnChange()">
                                @if (Model.Years == "2019")
                                {
                                    <option value="2019" selected="selected">@Html.GetTansByLan("packet-packetList-08", "2019 years", Lan)</option>}
                                else
                                {
                                    <option value="2019">@Html.GetTansByLan("packet-packetList-08", "2016 years", Lan)</option>}
                                @if (Model.Years == "2018")
                                {
                                    <option value="2018" selected="selected">@Html.GetTansByLan("packet-packetList-06", "2018 years", Lan)</option> }
                                else
                                {
                                    <option value="2018">@Html.GetTansByLan("packet-packetList-06", "2018 years", Lan)</option>
                                }
                                @if (Model.Years == "2017")
                                {
                                    <option value="2017" selected="selected">@Html.GetTansByLan("packet-packetList-07", "2017 years", Lan)</option>}
                                else
                                {
                                    <option value="2017">@Html.GetTansByLan("packet-packetList-07", "2017 years", Lan)</option>}
                            </select>
                            <div class="arrow-btm"></div>
                        </div>
                    </div>
                    <div class="draw-packet">
                        <div class="draw-packet-inner">
                            <div class="rowitem-ul rowitem-has-operate">

                                @if (sendList != null && sendList.Count > 0)
                                {
                                    foreach (var item in sendList)
                                    {
                                        <div class="rowitem">
                                            <div class="rowmd">
                                                @if (item.UseTypeInt == 0)
                                                {
                                                    <div class="nametxt">@Html.GetTansByLan("packet-packetList-14", "Random Amount", Lan)</div>
                                                }
                                                else if (item.UseTypeInt == 1)
                                                {
                                                    <div class="nametxt">@Html.GetTansByLan("packet-packetList-15", "Identical Amount", Lan)</div>
                                                }
                                                <div class="datenub">@item.CreateTimeDatetime.ToString("MM-dd HH:mm")</div>
                                            </div>
                                            <div class="rowft"><div class="distrib text-dark">@item.AmountDecimal.ToString("0.00") @Html.GetTansByLan("packet-silk-alias-01", "SILK", Lan)<div class="linefeed"><span class="text-gray">@Html.GetTansByLan("packet-packetList-16", "opened", Lan) <span class="text-dark">@item.CountUsedInt</span>/<span>@item.CountInt</span></span></div></div></div>
                                        </div>
                                        <div class="rowitem">
                                            <div class="rowmd text-right">
                                                @if (item.CountInt > item.CountUsedInt && item.ValidityTimeDatetime >= DateTime.Now)
                                                {
                                                    <a class="btn btn btn-outline-danger m-btn--pill btn-sm" href="javascript:;" onclick="GetSharePage('@item.PackedIdNvarchar')" id="">@Html.GetTansByLan("packet-packetList-17", "Copy address", Lan)</a>
                                                }
                                                &nbsp;
                                                <a class="btn btn btn-outline-danger m-btn--pill btn-sm" href="/Affirm/GetPacketIndex?OnlyCode=@item.PackedIdNvarchar&from=1&userId=@UserId&lan=@Lan">@Html.GetTansByLan("packet-packetList-18", "View details", Lan)</a>
                                            </div>
                                        </div>
                                    }
                                }
                                else
                                {
                                    <div class="rowitem-none">
                                        @Html.GetTansByLan("packet-packetList-10", "No data available", Lan)
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <form action="/Affirm/GetPacketIndex/" method="POST" id="J_modal_add_purchase">
                <input type="hidden" id="OnlyCode" name="OnlyCode" value="" />
                <input type="hidden" name="Lan" value="@(Lan)" />
            </form>
        }
    </div>
</div>
<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"></script>
<script>
    function OnChange() {
        var url = "/Affirm/GetPacketDetail" + "?type=" + $("#lucky-money-type").text() + "&years=" + $('.filter-control-select option:selected').val()+"&Lan=@(Lan)&userId=@UserId";
        window.location.href = url;
    }
    function GetSharePage(id) {
        $("#OnlyCode").val(id);
        $("#J_modal_add_purchase").attr("action", "/Packet/" + id);
        $("#J_modal_add_purchase").submit();
    }
</script>
</body>
</html>
