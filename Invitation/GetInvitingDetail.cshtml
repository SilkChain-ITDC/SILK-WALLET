@{
    Layout = null;
}<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>SilkChain</title>
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/css/vendors.stripped.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/style.bundle.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/main.css" rel="stylesheet" asp-append-version="true" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <!-- Google Tag Manager -->
    <script>(function (w, d, s, l, i) {w[l] = w[l] || []; w[l].push({'gtm.start':new Date().getTime(), event: 'gtm.js'}); var f = d.getElementsByTagName(s)[0], j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);})(window, document, 'script', 'dataLayer', 'GTM-5Q7VH5Q');</script>
</head>
<style>
.btn-danger{color:#fff;background-color:#e13c23;border-color:#e13c23;}
.btn-danger:hover{color:#fff;background-color:#f22d4e;border-color:#f12143;}
.btn-danger:focus, .btn-danger.focus{box-shadow:0 0 0 3px rgba(244, 81, 108, 0.5);}
.btn-danger.disabled, .btn-danger:disabled{background-color:#e13c23;border-color:#e13c23;}
.btn-danger:active, .btn-danger.active {background-color:#f22d4e;background-image:none;border-color:#f12143;}
.btn-lg, .btn-group-lg > .btn {padding:0.65rem 1.75rem;}
.m-grid.m-grid--ver-desktop.m-grid--desktop { display: flex; flex-direction: row; }
.m-grid.m-grid--ver-desktop.m-grid--desktop > .m-grid__item.m-grid__item--fluid { flex: 1 0 auto; }
.m-packet__wrapper {width: 430px; margin: 0 auto; }
.m-stack.m-stack--flex-general .m-packet__wrapper { flex: 1 0 auto; }
.m-packet__wrapper.invitebox {position:relative;background:url(/images/ba-active/red-packet-coupon@(Html.GetLang()=="en"? "-en" : "").jpg) no-repeat center top;background-size:contain;}
.invite-wrap {position:absolute;left:10%;top:0;width:80%;margin-top:75%;padding:0 1.5rem 1.5rem;}
.invite-wrap .coupon-box {background:url(data:image/png;base64,) no-repeat;background-size:contain;}
.invite-wrap .coupon-inn {position:relative;padding-top:37.5%;}
.invite-wrap .coupon-in2 {position:absolute;left:4%;top:6%;width:92%;height:88%;border:1px dashed #f0132d;}
.invite-wrap .coupon-tx1 {text-align:center;color:#f0132d;font-size:1.15rem;font-weight:bold;margin-top:10px;margin-bottom:4px;}
.invite-wrap .coupon-tx2 {text-align:center;color:#333;font-size:0.92rem;}
.invite-wrap .coupon-pull {position:absolute;width:100%;left:0;bottom:-1px;}
.invite-wrap .coupon-pull .getsilk {display:block;margin:0 -1px;background-color:#f0132d;color:#fff;font-size:1.15rem;padding:5px;text-align:center;text-decoration:none;}
.invite-wrap .inth-rules strong {color:#fdd772;font-weight:bold;}
.invite-wrap .inth-rules {margin-top:40px;color:#fff4c7;margin-left:-20px;margin-right:-20px;}
.invite-wrap .inth-rules dt {font-size:16px;margin-bottom:6px;}
.invite-wrap .inth-rules dt:before {content:"";float:left;margin-top:2px;margin-right:6px;width:20px;height:20px;background:url(data:image/png;base64,) no-repeat;background-size:cover;}
.invite-wrap .inth-rules dd {font-size:0.875rem;line-height:1.2;margin-bottom:4px;}
.m-packet__wrapper .resolved {position:absolute;width:100%;top:0;margin-top:168%;padding:10px;font-size:1rem;color:#fff;text-align:center;}
.m-packet__wrapper.invitebox .lang-switch {position:absolute;left:0;top:0;z-index:19;width:100%;padding:5px 10px;color:#fff;text-align:right;}
.m-packet__wrapper.invitebox .lang-switch > span {cursor:pointer;}
@@media (max-width:576px) {
    .m-packet__wrapper {width:100%;}
}

.modal-open .modal.show {display:-webkit-flex !important;-webkit-justify-content:center;-webkit-align-items:center;display:-ms-flex !important;-ms-justify-content:center;-ms-align-items:center;display:flex !important;justify-content:center;align-items:center;}
.modal .modal-fields .modal-content {height:inherit;background-color:#eee;border-radius:6px;}
.modal .modal-fields .modal-content .modal-header {position:absolute;top:0;right:0;z-index:9;border-bottom:none;padding:12px 10px 0 0;}
.modal .modal-fields .modal-content .modal-header .close {color:#666;opacity:1;height:18px;cursor:pointer;}
.modal .modal-fields .modal-content .has-danger {margin-top:-10px;color:#f4516c;}
.modal .modal-fields .modal-content .m-login__form-action {margin-top:2rem;}
.modal .modal-fields .modal-content .m-form .m-form__group + .m-form__group {padding-top:0;}
@@media (min-width:576px) {
    .modal-dialog.modal-fields {max-width:360px;width:360px;}
}
</style>
<body class="m--skin- m-header--static m-header--static-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default">
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop" style="height:inherit;background-color:#152659;">
            <div class="m-packet__wrapper invitebox">
                <div class="lang-switch">
                    @if (Html.GetLang() == "en")
                    {
                        <span class="ft12" id="chinesesLanguage"><img src="/images/chinese-language.png" /> <span style="vertical-align:middle;">中文</span></span>
                    }
                    else
                    {
                        <span class="ft12" id="englishLanguage"><img src="/images/english-language.png" /> <span style="vertical-align:middle;">English</span></span>
                    }
                </div>
                <div class="invite-wrap">
                    <div class="coupon-box">
                        <div class="coupon-inn">
                            <div class="coupon-in2">
                                <div class="coupon-tx1">免费新用户邀请红包券</div>
                                <div class="coupon-tx2">使用期限：领券后7天内有效</div>
                                <div class="coupon-pull"><a class="getsilk" href="#">立即领取</a></div>
                            </div>
                        </div>
                    </div>
                    <dl class="inth-rules">
                        <dt><strong>活动说明</strong></dt>
                        <dd><strong><em>1</em>.</strong> 领取免费新用户邀请红包券后，可以发送次红包邀请好友进行注册；</dd>
                        <dd><strong><em>2</em>.</strong> 每个小伙伴领取红包并完成注册KYC后，分享人即可获赠等额Silk积分奖励；</dd>
                        <dd><strong><em>3</em>.</strong> 每个链接仅限新用户注册领取，不限个数；</dd>
                        <dd><strong><em>4</em>.</strong> 每个链接会按领取顺序随机发放一个大额红包；</dd>
                        <dd><strong><em>5</em>.</strong> 红包领取人手机号/邮箱地址、提取Silk时输入的手机号/邮箱地址需为同一号码；</dd>
                        <dd><strong><em>6</em>.</strong> 其他未尽事宜，请详询客服。</dd>
                    </dl>
                </div>
                <div class="resolved">本活动最终解释权归Silk运营团队所有</div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalDrawSignin" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-fields" role="document">
            <div class="modal-content">
                <div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>
                <div class="modal-body">
                    <form class="m-login__form m-form" method="POST" action="/Account/ChkLogin" style="margin-top: 0;">
                        <input type="hidden" name="_token" value="">
                        @*
                        <input type="hidden" name="BackUrl" value="@Html.Raw(Model.BackUrl)" />
                        <input type="hidden" name="PacketID" value="@Html.Raw(Model.LuckMoneyInfo.PacketID)" />
                        *@
                        <div class="form-group m-form__group">
                            <input class="form-control m-input" type="text" placeholder="@Html.GetTansByLan("account-login-02", "Email/UserName")" name="UserName" id="UserName" value="" autocomplete="off" required autofocus>
                        </div>
                        <div class="form-group m-form__group">
                            <input class="form-control m-input" type="password" placeholder="@Html.GetTansByLan("account-login-03", "Password")" name="UserPwd" id="UserPwd" value="" autocomplete="off" required autofocus>
                        </div>
                        <div class="has-danger"></div>
                        <div class="m-login__form-action">
                            <input value="@Html.Raw(Html.GetTansByLan("account-login-06", "Sign In"))" type="submit" id="m_directly_submit" class="btn btn-block btn-danger m-btn--pill btn-lg" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

<script src="/js/vendors.stripped.min.js" type="text/javascript"></script>
<script src="https://cdn.bootcss.com/jquery.qrcode/1.0/jquery.qrcode.min.js"></script>
<script type="text/javascript">
$(function(){
    $('#modalDrawSignin').modal('show');
});
$("#chinesesLanguage").click(function () {
    setLang('cn');
    window.location.href = window.location.href;
});
$("#englishLanguage").click(function () {
    setLang('en');        
    window.location.href = window.location.href;
});
function setLang(lan)
{
    var exp = new Date();
    exp.setTime(new Date().getTime() + 365 * 24 * 60 * 60 * 1000);
    document.cookie = "lang=" + escape(lan) + ";expires=" + exp.toGMTString() + ";path=/;domain=silkchain.io";
}
</script>
</body>
</html>
