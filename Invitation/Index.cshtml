<script>

    function copyValToClipboard(element) {
        var $temp = $("<input>");
        $(element).parent().append($temp);
        $temp.val($(element).val()).select();
        document.execCommand("copy");
        $temp.remove();
        $(element).next().tooltip({
            title: 'Copy Success!',
            placement: 'top',
            trigger: 'click',
            delay: { "show": 500, "hide": 100 }
        }).tooltip('show');
        setTimeout(function () {
            $(element).next().tooltip('hide');
        }, 800);
        return false;
    }
    $(function () {
        $('input[type=file]').on('change', function (e) {
            $(this).parent().find(".upload_img").attr('src', '');
            $(this).parent().find(".upload_img").attr('src', window.URL.createObjectURL($(this)[0].files[0]));
            $(this).parent().find(".upload_img").css("display", "block");
            $(this).parent().find(".default_img").css("display", "none");
            $(this).parent().find(".file_label").css("display", "none");

            if ($(this)[0].name == "photoFrontSide") {
                $("#isChangeFront").val('true');
            }
        });
        $("#but_final_ok").click(function () {
            $("#txt_chk_user").val("");
            location.href = "/Account/Login";
        });

    });
</script>
<script type="text/javascript">
    var coin_price_range = @Html.Raw(JsonConvert.SerializeObject(coin_price_range));
    var buy_silk = {
        params: { eth_silk_rate: @(Math.Round(eth_silk_rate)), url: '/Invitation/CheckInvitation', IsFixPrice: false, FixPrice: 0, LockDays: 0, RealTimePrice: 0 },
        inItControl: function () {
            var reg_num = /^\d+(\.\d+)?$/;
            var error_num_tag = $("#ezt_withdraw_error"), eth_count_tag = $("#lblEztRemaining");
            $("#ezt_amount").keyup(function () {
                var eth_val = $(this).val();
                var coin_type = $("input[type=radio][name=CoinType]:checked").val();
                var chk_range_price = buy_silk.chk_price_range(reg_num, coin_type, eth_val);
                if (reg_num.test(eth_val)) {
                    var silk_count_num = Math.ceil(eth_val * buy_silk.params.eth_silk_rate);
                    var silk_num = new BigNumber((silk_count_num).toString());
                    $("#hd_silk_num").val(silk_count_num);
                    eth_count_tag.val(silk_num.toFormat());
                }
                if (chk_range_price.ok) {
                    error_num_tag.hide();
                }else {
                    error_num_tag
                        .find(".lb_coin_type").html(coin_type).parent()
                        .find(".lb_min_price").html(chk_range_price.min_p).parent()
                        .find(".lb_max_price").html(chk_range_price.max_p).parent()
                        .show();
                }
            }).blur(function () { $(this).keyup(); });

            $("#btn_ezt_withdraw").click(function () {
                var eth_tag = $("#ezt_amount");
                var coin_type = $("input[type=radio][name=CoinType]:checked").val();
                var chk_range_price = buy_silk.chk_price_range(reg_num, coin_type, eth_tag.val());
                var change_front = $("#isChangeFront").val();
                if (chk_range_price.ok) {
                    if (change_front == "false") {
                        swal("Upload photos");
                        return;
                    }
                    $("#RealTimePrice").val(buy_silk.params.RealTimePrice);
                    var url_params = new FormData($("#form_buy_silk").get(0));
                    buy_silk.request_file(buy_silk.params.url, url_params, { Option: 'ApplySilk' });
                }
            });

            $("#but_chk_user").click(function () {
                var code = $("#txt_chk_user").val();
                if ($.trim(code) == "") {
                    swal("Input invitation code");
                    return;
                }

                var params = { Option: 'GetUser', InviteCode: code };
                buy_silk.request_data(buy_silk.params.url, params, { Option: 'GetUser' });
            });
            if (@item.Step==3) {
                $("#img_qr_silk_address").empty().qrcode({ width: 240, height: 240, text: '@(item.Address)' });
            }
        },
        request_file: function (url, params, opt) {
            $.ajax({
                url: url,
                data: params,
                type: 'post',
                enctype: "multipart/form-data",
                processData: false,
                contentType: false,
                cache: false,
            }).done(function (data) {
                $(".m-loader").hide();
                if (data.result) {
                    switch (opt.Option) {
                        case "ApplySilk":
                            location.reload();
                            break;
                        default:
                            break;
                    }
                } else {
                    swal({
                        text: data.message,
                        type: data.result ? "success" : "error",
                    }).then(function (value) {
                    });
                }
            }).fail(function (xhr, status, error) {
                //alert("system error");
            });
        },
        request_data: function (url, params) {
            $.ajax({
                url: url,
                data: params,
                type: 'post',
                beforeSend: function () { $("#btn_ezt_withdraw").attr("disabled", true); },
                complete: function () { $("#btn_ezt_withdraw").attr("disabled", false); },
            }).done(function (data) {
                $(".m-loader").hide();
                if (data.result) {
                    switch (params.Option) {
                        case "GetUser":
                            location.reload();
                            break;
                        case "GetStatus":
                            if (data.data.Status == 4) {
                                $("#div_result").html("Payment succeed").parent().show();
                            } else {
                                var data = { Option: 'GetStatus', "ID": data.data.ID };
                                var index = $("#js_zx-carousel").find(".carousel-item").filter(".active").index();

                                setTimeout(function () {
                                    buy_silk.request_data(buy_silk.params.url, data);
                                }, 2000);
                            }
                            break;
                        default:
                            break;
                    }
                } else {
                    swal({
                        text: data.message,
                        type: data.result ? "success" : "error",
                    }).then(function (value) {
                    });
                }
            }).fail(function (xhr, status, error) {
                //alert("system error");
            });
        },
        show_pay_address: function (obj) {
            var address = $(obj).attr("data-address");
            var coin_type = $(obj).attr("data-cointype");
            var amount = $(obj).attr("data-dealamount");
            $("#sp_payment").html(amount + " " + coin_type);
            $("#deposit_silk_address").val(address);
            $("#m_eth_address").modal('show');
        },
        cancel_order: function (obj) {
            var id = $(obj).attr("data-id");
            swal({
                title: "Are you sure?",
                text: "Your will Cancellation of order!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            }).then((willDelete) => {
                if (willDelete) {
                    var url_params = { Option: 'CancelOrder', "ID": id };
                    buy_silk.request_data(buy_silk.params.url, url_params);
                } else {
                }
            });
        },
        bind_info: function (data) {
            var rate = data.Rate;
            buy_silk.params.IsFixPrice = data.IsFixPrice;
            buy_silk.params.FixPrice = data.FixPrice;
            buy_silk.LockDays = data.LockDays;
            if (rate != "" && rate.length > 0) {
                $.each(rate, function (i, n) {
                    if (n.CoinType == "ETH") {
                        buy_silk.params.RealTimePrice = n.Price;
                    }
                });
            }
            $("#sp_user_name").html(data.UserName);
            buy_silk.params.eth_silk_rate = buy_silk.params.IsFixPrice ? buy_silk.params.FixPrice : buy_silk.params.RealTimePrice;
            $("#sp_price_type").html(buy_silk.params.IsFixPrice ? "Specified Price" : "Real-time Price");
            var silk_num = new BigNumber((Math.round(buy_silk.params.eth_silk_rate)).toString());
            $(".sp_ethtosilk").html(silk_num.toFormat());
            $("#sp_frozen_day").html(data.LockDays);
        },
        show_payment: function (data) {
            $("#sp_payment").html(data.Amount + " " + data.CoinType);
            $("#deposit_silk_address").val(data.Address);
            $('#js_zx-carousel').carousel('next');
            $("#img_qr_silk_address").empty().qrcode({ width: 240, height: 240, text: data.Address });
            $("#deposit_silk_address").val(data.Address);
        },
        chk_price_range: function (regx, coin_type, val) {
            var temp_price = { ok: false, min_p: '', max_p: '' };
            $.each(coin_price_range, function (i, n) {
                if (n.CoinType == coin_type) {
                    temp_price.min_p = n.MinPrice;
                    temp_price.max_p = n.MaxPrice;
                    if (regx.test(val) && (val >= n.MinPrice && val <= n.MaxPrice)) {
                        temp_price.ok = true;
                    }
                    return false;
                }
            });
            return temp_price;
        }
    };
    $(function () {
        buy_silk.inItControl();
    });
</script>
