        <script type="text/javascript">
            function changeMenuItem() {
                if (document.documentElement.clientWidth < 768) {
                    $(".m-dropdown-item").removeClass("m-dropdown--align-center");
                    $(".m-dropdown-item").addClass("m-dropdown--align-left");
                    $(".m-dropdown__arrow-item").removeClass("m-dropdown__arrow--center");
                    $(".m-dropdown__arrow-item").addClass("m-dropdown__arrow--left");
                } else {
                    $(".m-dropdown-item").removeClass("m-dropdown--align-left");
                    $(".m-dropdown-item").addClass("m-dropdown--align-center");
                    $(".m-dropdown__arrow-item").removeClass("m-dropdown__arrow--left");
                    $(".m-dropdown__arrow-item").addClass("m-dropdown__arrow--center");
                }
            }
            $(function () {
                setNavigation();
                changeMenuItem();
            });

            function setNavigation() {
                var path = window.location.pathname;
                path = path.replace(/\/$/, "");
                path = decodeURIComponent(path);

                $(".m-menu__item a").each(function () {
                    var href = $(this).attr('href');
                    if (href.indexOf(path) != -1) {
                        $(this).closest('li').addClass('m-menu__item--active');
                        $(this).closest('li').closest('ul').closest('li').addClass('m-menu__item--open m-menu__item--expanded');
                    }
                });
            }

            $(window).resize(function () {
                changeMenuItem();
            });

            function onDepositBtc() {
                $.ajax({
                    url: "https://wallet.silkchain.io/wallet/btcaddress",
                }).done(function (data) {
                    $("#img_qr_btc_address").attr('src', "https://wallet.silkchain.io/wallet/qr/" + data);
                    $("#tx_btc_address").val(data);
                    $("#m_deposit_btc").modal("show");
                    if (typeof analytics !== "undefined") { analytics.track("Deposit", { currency: "BTC" }); }
                });
            }
            function onDepositEth() {
                $.ajax({
                    url: "https://wallet.silkchain.io/wallet/ethaddress",
                }).done(function (data) {
                    $("#img_qr_eth_address").attr('src', "https://wallet.silkchain.io/wallet/qr/" + data);
                    $("#tx_eth_address").val(data);
                    $("#m_deposit_eth").modal("show");
                    if (typeof analytics !== "undefined") { analytics.track("Deposit", { currency: "ETH" }); }
                });
            }
            function copyValToClipboard(element) {
                var $temp = $("<input>");
                $(element).parent().append($temp);
                $temp.val($(element).val()).select();
                document.execCommand("copy");
                $temp.remove();
                return false;
            }
            $("input[type=text], textarea").on({
                'touchstart': function () {
                    zoomDisable();
                }
            });
            $("input[type=text], textarea").on({
                'touchend': function () {
                    setTimeout(zoomEnable, 500);
                }
            });
            function zoomDisable() {
                $('head meta[name=viewport]').remove();
                $('head').prepend('<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0" />');
            }
            function zoomEnable() {
                $('head meta[name=viewport]').remove();
                $('head').prepend('<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=1" />');
            }
        </script>
